{
    "name": "gregnuj/fondant",
    "description": "Fondant plugin for CakePHP 3",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "fondant"],
    "homepage": "https://github.com/gregnuj/fondant",
    "license": "MIT",
    "authors": [
        {
           "name": "Greg Junge"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "cakephp/cakephp": "^3.6.0",
        "cakephp/plugin-installer": "^1.0",
        "wyrihaximus/twig-view": "^4.3.4",
        "koala-framework/composer-extra-assets": "^2.0"
    },
    "require-dev": {
        "cakephp/cakephp-codesniffer": "^3.0",
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Fondant\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fondant\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "extra": {
        "require-bower": {
            "jquery": "*"
        }
    }
}

